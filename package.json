{
	"name": "eddi",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:start": "docker compose up",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:seed": "npx tsx --env-file=.env ./src/lib/server/db/seed",
		"db:seed:fresh": "npx tsx --env-file=.env ./src/lib/server/db/seed -- --fresh",
		"curriculum:update": "npx tsx ./src/lib/server/db/seed/data/generate-curriculum-data.ts",
		"db:studio": "drizzle-kit studio",
		"db:diagram": "tsx ./src/lib/server/db/dbml.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
