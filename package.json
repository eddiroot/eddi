{
  "name": "eddi",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "curriculum:update": "npx tsx ./src/lib/server/db/seed/data/generate-curriculum-data.ts",
    "db": "sst shell drizzle-kit",
    "db:migrate": "sst shell drizzle-kit migrate",
    "db:push": "sst shell drizzle-kit push",
    "db:seed": "sst shell tsx src/lib/server/db/seed",
    "db:diagram": "tsx ./src/lib/server/db/dbml.ts",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview",
    "sso": "aws sso login --sso-session=eddi"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
